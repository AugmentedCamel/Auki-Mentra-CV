{
  "name": "@augmentos/mauki",
  "version": "0.1.0",
  "private": true,
  "description": "test app for mauki",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "bun --hot src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "docker:dev": "docker compose -f docker/docker-compose.dev.yml -p dev up",
    "docker:dev:detach": "docker compose -f docker/docker-compose.dev.yml -p dev up -d",
    "docker:stop": "docker compose -f docker/docker-compose.dev.yml -p dev down",
    "docker:build": "docker compose -f docker/docker-compose.dev.yml -p dev build",
    "prod": "docker compose -f docker/docker-compose.yml up",
    "prod:detach": "docker compose -f docker/docker-compose.yml up -d",
    "prod:stop": "docker compose -f docker/docker-compose.yml down",
    "prod:build": "docker compose -f docker/docker-compose.yml build",
    "logs": "docker logs -f dev-web-1",
    "restart": "docker restart dev-web-1",
    "exec": "docker exec -it dev-web-1",
    "sh": "docker exec -it dev-web-1 sh",
    "add": "docker exec -it dev-web-1 npm install",
    "add:dev": "docker exec -it dev-web-1 npm install -D",
    "remove": "docker exec -it dev-web-1 npm uninstall",
    "test:container": "docker exec -it dev-web-1 npm test",
    "lint:container": "docker exec -it dev-web-1 npx eslint 'src/**/*.ts'",
    "image:build": "docker build -t augmentos/mauki:latest -t augmentos/mauki:$npm_package_version -f docker/Dockerfile .",
    "image:push": "docker push augmentos/mauki:latest && docker push augmentos/mauki:$npm_package_version",
    "image:release": "npm run image:build && npm run image:push",
    "ghcr:build": "docker build -t ghcr.io/augmentos/mauki:latest -t ghcr.io/augmentos/mauki:$npm_package_version -f docker/Dockerfile .",
    "ghcr:push": "docker push ghcr.io/augmentos/mauki:latest && docker push ghcr.io/augmentos/mauki:$npm_package_version",
    "ghcr:release": "npm run ghcr:build && npm run ghcr:push"
  },
  "dependencies": {
    "@mentra/sdk": "^2.1.29",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.17",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18 <=22"
  },
  "license": "MIT"
}
